{
  "dependencies": {
    "electron-builder-squirrel-windows": "^24.2.0",
    "electron-log": "^4.4.8",
    "node-addon-api": "^6.0.0",
    "node-machine-id": "^1.1.12",
    "npcap": "file:./node_pcap",
    "photon-packet-parser": "file:./photon-packet-parser"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "main": "main.js",
  "devDependencies": {
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0"
  },
  "build": {
    "productName": "Rtadar",
    "appId": "com.tisoz.tradar",
    "icon": "icon_512.png",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "nsis"
    },
    "files": [
      "**/*",
      "npcap_img/npcap.exe",
      "!photon-packet-parser/classes/PhotonCommand.js"
    ],
    "extraResources": [
      {
        "from": "npcap_img/",
        "to": "../npcap_img/",
        "filter": [
          "npcap.exe"
        ]
      }
    ]
  },
  "version": "1.8.2",
  "name": "Rtdar"
}
